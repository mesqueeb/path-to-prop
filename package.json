{
  "name": "path-to-prop",
  "version": "1.0.0",
  "sideEffects": false,
  "description": "Retrieves a property from an object based on a 'path/to.that.prop'",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "types/index.d.ts",
  "scripts": {
    "test": "ava --verbose",
    "build": "rm -rf ./types && rm -rf ./dist && npm run lint && npm run test && npm run rollup",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "rollup": "rollup -c build/rollup.js",
    "release": "npm run build && np"
  },
  "keywords": [
    "path-to-prop",
    "dot-prop",
    "dotprop",
    "propdot",
    "dot-notation",
    "dotnotation-prop",
    "delve",
    "prop-at-path",
    "get-prop-at-path",
    "get-property-at-path",
    "get-dot-prop",
    "get-dot-property"
  ],
  "author": "Luca Ban - Mesqueeb",
  "funding": "https://github.com/sponsors/mesqueeb",
  "license": "MIT",
  "homepage": "https://github.com/mesqueeb/path-to-prop#readme",
  "dependencies": {},
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "ava": "^4.0.1",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-tree-shaking": "^1.10.0",
    "np": "^7.6.0",
    "prettier": "^2.5.1",
    "rollup": "^2.66.0",
    "rollup-plugin-typescript2": "^0.31.1",
    "tsconfig-paths": "^3.12.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  },
  "bugs": {
    "url": "https://github.com/mesqueeb/path-to-prop/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mesqueeb/path-to-prop.git"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "tsconfig-paths/register",
      "ts-node/register"
    ]
  },
  "np": {
    "yarn": false,
    "branch": "production"
  }
}
